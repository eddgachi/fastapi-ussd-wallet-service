# Environment
ENV=development

# Database
DATABASE_URL=postgresql+psycopg2://fastapi_user:fastapi_password@db/coffee_traceability


# JWT
SECRET_KEY=your-super-secret-key-change-this-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_DIR=uploads
QR_CODE_DIR=qrcodes

# Security
BCRYPT_ROUNDS=12

# Celery
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/1

# Redis
REDIS_URL=redis://redis:6379/0

# Application
APP_NAME=
APP_VERSION=1.0.0

# CORS
CORS_ORIGINS=["*"]